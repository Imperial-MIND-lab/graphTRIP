{
  "seed": [0],
  "save_weights": [false],
  "lr": [0.001],
  "num_epochs": [300],
  "num_z_samples": [1, 3],
  "alpha": [0.4, 0.5],
  "dataset": {
    "study": ["psilodep2"],
    "session": ["before"],
    "atlas": ["schaefer100"],
    "target": ["QIDS_Final_Integration"],
    "batch_size": [7],
    "num_folds": [6],
    "val_split": [0.0],
    "graph_attrs": [["QIDS_Before", "BDI_Before", "Condition", "Stop_SSRI"]],
    "context_attrs": [["Condition"]],
    "node_attrs": [["5-HT1A_Believeau-3", "5-HT2A_Believeau-3", "5-HTT_Believeau-3"]],
    "edge_attrs": [["functional_connectivity"]],
    "edge_tfm_type": ["DensityThresholdAdjacency"],
    "edge_tfm_params": {
      "density": [0.2],
      "edge_info": ["functional_connectivity"]
    },
    "add_3Dcoords": [true],
    "self_loop_fill_value": [1],
    "standardise_x": [false]
  },
  "mlp_model": {
    "model_type": ["RegressionMLP"],
    "params": {
      "dropout": [0.25],
      "hidden_dim": [64, 128],
      "layernorm": [false],
      "mse_reduction": ["sum"],
      "num_layers": [3, 4],
      "output_dim": [1],
      "reg_strength": [0.01]
    }
  },
  "vgae_model": {
    "model_type": ["NodeLevelVGAE"],
    "params": {
      "dropout": [0.25],
      "latent_dim": [32, 64],
      "node_emb_dim": [32, 64],
      "reg_strength": [0.01]
    },
    "node_emb_model_cfg": {
      "model_type": ["NodeEmbeddingGATv2Conv_withSkip"],
      "params": {
        "hidden_dim": [32, 64],
        "layernorm": [true],
        "num_heads": [2, 4],
        "num_layers": [6]
      }
    },
    "encoder_cfg": {
      "model_type": ["DenseOneLayerEncoder"]
    },
    "edge_decoder_cfg": {
      "model_type": ["MLPEdgeDecoder"],
      "params": {
        "act": ["tanh"],
        "hidden_dim": [32],
        "num_layers": [3]
      }
    },
    "edge_idx_decoder_cfg": {
      "model_type": ["MLPEdgeDecoder"],
      "params": {
        "act": ["sigmoid"],
        "hidden_dim": [32],
        "num_layers": [3]
      }
    },
    "node_decoder_cfg": {
      "model_type": ["MLPNodeDecoder"],
      "params": {
        "hidden_dim": [32],
        "num_layers": [3]
      }
    },
    "pooling_cfg": {
      "model_type": ["AttentionNetPooling"],
      "params": {
        "hidden_dim": [32],
        "reduce": ["mean"]
      }
    }
  }
}