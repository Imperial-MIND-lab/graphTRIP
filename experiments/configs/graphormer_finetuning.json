{
    "seed": [0],
    "save_weights": [false],
    "weights_dir": ["outputs/graphTRIP_GRAPHORMER/pretrain_for_psilodep1/job_0"],
    "output_dir": ["outputs/graphTRIP_GRAPHORMER/finetune_on_psilodep1"],
    "num_epochs": [30, 50, 100],
    "num_z_samples": [1],
    "alpha": [0.0],
    "vgae_lr": [0.0],
    "mlp_lr": [0.001],
    "rho": [0.4, 0.5],
    "leak_rate": [0.9, 0.95],
    "dataset": {
      "study": ["psilodep1"],
      "session": ["before"],
      "atlas": ["schaefer100"],
      "target": ["QIDS_1week", "QIDS_3months"],
      "batch_size": [7],
      "num_folds": [7],
      "val_split": [0.0],
      "graph_attrs": [
        ["QIDS_Before", "BDI_Before"]],
      "context_attrs": [[]],
      "node_attrs": [["5-HT1A_Believeau-3", "5-HT2A_Believeau-3", "5-HTT_Believeau-3"]],
      "edge_attrs": [["functional_connectivity"]],
      "edge_tfm_type": ["DensityThresholdAdjacency"],
      "edge_tfm_params": {
        "edge_info": ["functional_connectivity"],
        "density": [0.2]
      },
      "add_3Dcoords": [true],
      "self_loop_fill_value": [1],
      "standardise_x": [false]
    },
    "mlp_model": {
      "model_type": ["RegressionMLP"],
      "params": {
        "dropout": [0.25],
        "hidden_dim": [128],
        "layernorm": [false],
        "mse_reduction": ["sum"],
        "num_layers": [4],
        "output_dim": [1],
        "reg_strength": [0.01]
      }
    },
    "vgae_model": {
      "model_type": ["NodeLevelVGAE"],
      "params": {
        "dropout": [0.25],
        "latent_dim": [64],
        "node_emb_dim": [64],
        "reg_strength": [0.01]
      },
      "node_emb_model_cfg": {
        "model_type": ["NodeEmbeddingGraphormer"],
        "params": {
          "hidden_dim": [32],
          "layernorm": [true],
          "num_heads": [2],
          "num_layers": [4]
        }
      },
      "encoder_cfg": {
        "model_type": ["DenseOneLayerEncoder"]
      },
      "edge_decoder_cfg": {
        "model_type": ["MLPEdgeDecoder"],
        "params": {
          "act": ["tanh"],
          "hidden_dim": [32],
          "num_layers": [3]
        }
      },
      "edge_idx_decoder_cfg": {
        "model_type": ["MLPEdgeDecoder"],
        "params": {
          "act": ["sigmoid"],
          "hidden_dim": [32],
          "num_layers": [3]
        }
      },
      "node_decoder_cfg": {
        "model_type": ["MLPNodeDecoder"],
        "params": {
          "hidden_dim": [32],
          "num_layers": [3]
        }
      },
      "pooling_cfg": {
        "model_type": ["GraphTransformerPooling"],
        "params": {
          "reduce": ["mean"],
          "num_heads": [4],
          "ff_hidden_dim": [64]
        }
      }
    }
}